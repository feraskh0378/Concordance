<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <!-- Home servlet -->
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.concordance.Home.HomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    
    <!-- Tabs servlet -->
    <servlet>
        <servlet-name>DocumentsServlet</servlet-name>
        <servlet-class>com.concordance.Tabs.DocumentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocumentsServlet</servlet-name>
        <url-pattern>/DocumentsTable</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>WordsServlet</servlet-name>
        <servlet-class>com.concordance.Tabs.WordsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WordsServlet</servlet-name>
        <url-pattern>/WordsTable</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>DBWordsServlet</servlet-name>
        <servlet-class>com.concordance.Tabs.DBWordsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DBWordsServlet</servlet-name>
        <url-pattern>/DBWordsTable</url-pattern>
    </servlet-mapping>
    
    
    
    
     <servlet>
        <servlet-name>GroupsServlet</servlet-name>
        <servlet-class>com.concordance.Tabs.GroupsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GroupsServlet</servlet-name>
        <url-pattern>/GroupsTable</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>WordsTextServlet</servlet-name>
        <servlet-class>com.concordance.Tabs.WordsTextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WordsTextServlet</servlet-name>
        <url-pattern>/WordsText</url-pattern>
    </servlet-mapping>
    
    
    

    <!-- About servlet -->
    <servlet>
        <servlet-name>AboutServlet</servlet-name>
        <servlet-class>com.example.AboutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AboutServlet</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>

    <!-- Contact servlet -->
    <servlet>
        <servlet-name>ContactServlet</servlet-name>
        <servlet-class>com.example.ContactServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContactServlet</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>

</web-app>
